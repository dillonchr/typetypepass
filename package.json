{
  "private": true,
  "name": "type-type-pass",
  "version": "0.0.0",
  "license": "MIT",
  "homepage": "https://dillonchristensen.com/typetypepass/",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --template src/template.html --no-prerender --clean",
    "serve": "preact build && preact serve",
    "dev": "preact watch --template src/template.html",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-synacor": "^2.0.2",
    "if-env": "^1.0.4",
    "preact-cli": "^2.2.1"
  },
  "dependencies": {
    "icono": "^1.3.1",
    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "preact-redux": "^2.0.3",
    "preact-router": "^2.5.7",
    "redux": "^3.7.2",
    "socket.io-client": "^2.0.4"
  }
}
